%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% GCD (Euclid's algorithm)
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% GCD macro.
Let gcd be (@u p;@u p 2{;0;=;@p 4;?;@p 5;mod}).

% Compute the gcd of 48 and 180 and keep only the result.
(48;180;gcd;!);print.
